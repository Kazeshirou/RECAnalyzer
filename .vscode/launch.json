{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "RECAnalyzer unix O0 debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/RECAnalyzer",
            "args": [
                "--etf=../REC/rec-template.etf",
                "--eaf=./corpus/20081223-furekl-a1.eaf"
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build unix O0 debug",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "RECAnalyzer test O0 debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/test_RECAnalyzer",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build tests O0 debug",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}