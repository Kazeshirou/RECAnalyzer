{
    "target_folder": "bin/",
    "name": "test_analysis",
    "rec_template": "src/tests/rec/etf/test.etf",
    "convert": [
        {
            "type": "rec_template",
            "from": "src/tests/rec/etf/test.etf",
            "to": "bin/test_etf.json"
        }
    ],
    "transactions": [
        {
            "type": "time_slots",
            "output_file_extension": "json",
            "exclude_tiers": [
                "слой1"
            ],
            "exclude_annotations": [
                "тексе_цель_аннотация1"
            ],
            "files": [
                "src/tests/.*.eaf"
            ]
        },
        {
            "type": "window",
            "size": 100,
            "step_multiplier": 0.5,
            "output_file_extension": "csv",
            "tiers_settings": [
                {
                    "name": "tier1",
                    "mode": "part",
                    "value": 0.2
                }
            ],
            "annotations_settings": [
                {
                    "name": "ann1",
                    "mode": "any"
                }
            ],
            "exclude_tiers": [
                "слой1"
            ],
            "exclude_annotations": [
                "тексе_цель_аннотация1"
            ],
            "files": [
                "src/tests/.*.eaf"
            ]
        },
        {
            "type": "target_tier",
            "target_tier": "рот",
            "output_file_extension": "bin",
            "exclude_tiers": [
                "слой1"
            ],
            "exclude_annotations": [
                "тексе_цель_аннотация1"
            ],
            "files": [
                "src/tests/rec/eaf/test.eaf"
            ]
        }
    ],
    "sets": [
        {
            "output_file_extension": "bin",
            "transactions_files": [
                "file1",
                "file2"
            ],
            "exclude_tiers": [
                "слой1"
            ],
            "exclude_annotations": [
                "тексе_цель_аннотация1"
            ],
            "min_support": 0.0,
            "max_support": 0.99
        }
    ],
    "rules": [
        {
            "output_file_extension": "bin",
            "sets_files": [
                "file1",
                "file2"
            ],
            "exclude_tiers": [
                "слой1"
            ],
            "exclude_annotations": [
                "тексе_цель_аннотация1"
            ],
            "min_support": 0.0,
            "max_support": 0.99,
            "min_confidence": 0.0,
            "max_confidence": 0.99
        }
    ],
    "clustering": [
        {
            "output_file_extension": "bin",
            "exclude_tiers": [
                "слой1"
            ],
            "exclude_annotations": [
                "тексе_цель_аннотация1"
            ],
            "input_files": [
                "file1",
                "file2",
                "file3"
            ],
            "r": 2.0
        }
    ]
}